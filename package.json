{
  "name": "labstart-docs",
  "version": "0.1.0",
  "description": "Documentação automática para APIs Clean Architecture LabStart",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspaces",
    "test": "npm run test --workspaces",
    "lint": "eslint packages/*/src/**/*.ts",
    "clean": "rm -rf packages/*/dist && rm -rf packages/*/node_modules/.cache"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lab-Start-Desenvolvimento/labstart-docs.git"
  },
  "author": "LabStart - Vander Lima de Andrade",
  "license": "MIT"
}